{
  "manifest_version": 2,
  "name": "Friends Notifier",
  "version": "0.1",
  "permissions": [
    "notifications"
  ],
  "background": {
    "scripts": ["jquery-3.2.1.js","background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.js","content.js"]
    }
  ]
}
